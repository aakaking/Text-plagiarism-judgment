   "metadata": {},
   "outputs": [],
   "source": [
    "def clean(words):\n",
    "    clean_words = []\n",
    "    for word in words:\n",
    "        if not word.isdigit() and word not in stopwords and 1<len(word)<5:\n",
    "            clean_words.append(word)\n",
    "    return clean_words"
   ]
